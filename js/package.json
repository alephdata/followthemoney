{
  "name": "@alephdata/followthemoney",
  "version": "3.7.4",
  "description": "JavaScript version of the followthemoney data model",
  "keywords": [],
  "main": "dist/followthemoney.umd.js",
  "module": "dist/followthemoney.es5.js",
  "typings": "dist/types/followthemoney.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "OCCRP Data <data@occrp.org>",
  "repository": {
    "type": "git",
    "url": "https://github.com/alephdata/followthemoney"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint src",
    "prebuild": "rimraf dist && npm run build:icons",
    "build:icons": "node scripts/buildIcons.js",
    "build": "tsc --module commonjs && rollup -c rollup.config.mjs",
    "start": "npm run build:icons && rollup -c rollup.config.mjs -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@rollup/plugin-commonjs": "~28.0.0",
    "@rollup/plugin-json": "~6.1.0",
    "@rollup/plugin-node-resolve": "~15.3.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "colors": "~1.4.0",
    "cross-env": "~7.0.3",
    "eslint": "^9.11.1",
    "jest": "^29.7.0",
    "jest-config": "~29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prompt": "^1.3.0",
    "replace-in-file": "~8.2.0",
    "rimraf": "~6.0.1",
    "rollup": "^4.24.0",
    "shelljs": "^0.8.5",
    "svgo": "~3.3.2",
    "ts-jest": "^29.2.5",
    "ts-node": "~10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  },
  "dependencies": {
    "uuid": "~11.0.2"
  }
}
